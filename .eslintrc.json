{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true,
     "worker": true,
     "serviceworker": true
  },
  "rules": {
    "no-self-compare": 1,
    "no-restricted-globals": 0,
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ]
  }
}
